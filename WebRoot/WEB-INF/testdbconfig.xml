		<secondarycol>realName</secondarycol>
		<thirdcol>zhuanye</thirdcol>
	</table>
	<table name="t_teacher">
		<primarykey>id</primarykey>
		<secondarycol>realName</secondarycol>
		<thirdcol>yuanxi</thirdcol>
	</table>
	<table name="t_subject">
		<primarykey>id</primarykey>
